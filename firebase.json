{
  "hosting": {
    "public": "public",
    "cleanUrls": true,
    "trailingSlash": false,
    "redirects": [
      {
        "source": "/kalkulator",
        "destination": "/calculator",
        "type": 308
      },
      {
        "source": "/10GB",
        "destination": "https://fsn1-speed.hetzner.com/10GB.bin",
        "type": 308
      },
      {
        "source": "**/.git",
        "destination": "/10GB",
        "type": 307
      },
      {
        "source": "**/.env",
        "destination": "/10GB",
        "type": 307
      },
      {
        "source": "**/wp-login.php",
        "destination": "/10GB",
        "type": 307
      }
    ]
  }
}
